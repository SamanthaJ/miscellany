<div class="container">
  <div class="row">
    <div class="col-md-offset-3 col-md-6">
      <div class="posts-index">
        <h1>Blog Entries</h1>
        <% @posts.each do |post| %>
          <p id="post-links"><%= link_to post.title, post_path(post) %></p> 
        <% end %>
        <br/>  
        <%= link_to "Create a new post", new_post_path, class: "links" %>
      </div>
    </div>
  </div>
</div>
